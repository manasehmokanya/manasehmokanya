	  function convertNow(){
	  var Euro = 0.8288230000;
	  var USD =  0.1073160000;
	  var XAF = 0.8288230000;
	  var Pounds = 0.9341500000;
	  var YEN = 0.0064368300;
	  var cfrom = document.getElementById("currency-from").value;
      var cto  = document.getElementById("currency-to").value;
      var cnum = document.getElementById("currency-num").value;
	  var amount = cfrom * cto * cnum;
	  alert(amount);
	  document.getElementById("result").innerHTML = amount;
     }
